<application>
<name>Fingertips Data Dumper RIGHT</name>
<dependencies>
      <port>icub14</port>
</dependencies>


<!-- ****************************************************************************************************************** -->
<!-- ******************************************************************************** -->
<!-- ** Data Dumpers                                                                  -->
    <module>
        <name>dataDumper</name>
        <node></node>
        <parameters>--name /dump_right_ft --dir data/right/ft/ --rxTime</parameters>
        <workdir>/var/usr/fg/fgicub/iCub/tactileExploration/datadumper/</workdir>
        <tag>datadumper FT Values</tag>
    </module>
    
    <module>
        <name>dataDumper</name>
        <node></node>
        <parameters>--name /dump_right_pos --dir data/right/pos/ --rxTime</parameters>
        <workdir>/var/usr/fg/fgicub/iCub/tactileExploration/datadumper/</workdir>
        <tag>datadumper Finger Pos</tag>
    </module>
    
    <module>
        <name>dataDumper</name>
        <node></node>
        <parameters>--name /dump_right_wbd --dir data/right/wbd/ --rxTime</parameters>
        <workdir>/var/usr/fg/fgicub/iCub/tactileExploration/datadumper/</workdir>
        <tag>datadumper WBD values</tag>
    </module>
    
    <module>
        <name>dataDumper</name>
        <node></node>
        <parameters>--name /dump_right_skin_raw --dir data/right/skin/raw/ --rxTime</parameters>
        <workdir>/var/usr/fg/fgicub/iCub/tactileExploration/datadumper/</workdir>
        <tag>datadumper Skin values raw</tag>
    </module>   
    
    <module>
        <name>dataDumper</name>
        <node></node>
        <parameters>--name /dump_right_skin_comp --dir data/right/skin/comp/ --rxTime</parameters>
        <workdir>/var/usr/fg/fgicub/iCub/tactileExploration/datadumper/</workdir>
        <tag>datadumper Skin values compensated</tag>
    </module>   
    
    <module>
        <name>dataDumper</name>
        <node>icub14</node>
        <parameters>--name /dump_right_nano17 --dir data/right/nano17/ --rxTime</parameters>
        <workdir>/var/usr/fg/fgicub/iCub/tactileExploration/datadumper/</workdir>
        <tag>datadumper nano17 FT values</tag>
    </module>
<!-- ****************************************************************************************************************** -->
    
    
<!-- ****************************************************************************************************************** -->
<!-- ******************************************************************************** -->
<!-- ** Connections                                                                   -->
    <connection>
        <from>/icub/right_arm/state:o</from>
        <to>/dump_right_pos</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/icub/right_arm/analog:o</from>
        <to>/dump_right_ft</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/icub/skin/right_hand</from>
        <to>/dump_right_skin_raw</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/icub/skin/right_hand_comp</from>
        <to>/dump_right_skin_comp</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/wholeBodyDynamics/right_arm/cartesianEndEffectorWrench:o</from>
        <to>/dump_right_wbd</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/NIDAQmxReader/data/real:o</from>
        <to>/dump_right_nano17</to>
        <protocol>udp</protocol>
    </connection>
<!-- ****************************************************************************************************************** -->
</application>
